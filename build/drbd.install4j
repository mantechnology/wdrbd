                      <void property="fileFilterScript">
                        <object class="com.install4j.api.beans.ScriptProperty">
                          <void property="value">
                            <string>String filesetName =  fileInfo.getFilesetName();
if(filesetName.startsWith("Windows"))
{
    String osName;
    Float osVer = (Float)context.getVariable("os.floatver");
        
    if (osVer &gt;= 10.0f)
    {
        osName = "Windows 10 ";
    }
    else if (osVer &gt;= 6.3f)
    {
        osName = "Windows 8.1 ";
    }
    else if (osVer == 6.2f)
    {
        osName = "Windows 8 ";
    }
    else if (osVer == 6.1f)
    {
        osName = "Windows 7 ";
    }
    else
    {
        osName = "Windows Server 2008 ";
    }
    
    if(filesetName.contains(osName))
    {
        return true;
    }
    else
    {
        return false;
    }
}
else
{
    return true;
}</string>
